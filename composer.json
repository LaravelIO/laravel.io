{
    "name": "laravelio/laravel.io",
    "description": "The source code for the Laravel.io community portal.",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3",
        "algolia/algoliasearch-client-php": "^2.2",
        "algolia/scout-extended": "^1.9",
        "blade-ui-kit/blade-heroicons": "^0.1.0",
        "blade-ui-kit/blade-zondicons": "^0.1.2",
        "doctrine/dbal": "^2.5",
        "facade/ignition": "^2.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^7.0",
        "laravel/horizon": "^4.0",
        "laravel/socialite": "^4.3",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "lasserafn/php-initial-avatar-generator": "^2.0",
        "league/commonmark": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "livewire/livewire": "^1.0",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^3.7",
        "roave/security-advisories": "dev-master",
        "spatie/laravel-backup": "^6.8",
        "spatie/laravel-feed": "^2.6",
        "spatie/laravel-robots-middleware": "^1.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "barryvdh/laravel-debugbar": "^3.3",
        "fzaninotto/faker": "^1.9",
        "laravel/browser-kit-testing": "^6.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "resources/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "format": [
            "vendor/bin/php-cs-fixer fix"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "setup": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "php artisan key:generate",
            "php artisan migrate --seed",
            "npm install",
            "npm run dev"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
