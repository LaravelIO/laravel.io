{
    "description": "Laravel.IO",
    "require": {
        "laravel/framework": "4.1.*",
        "mccool/laravel-auto-presenter": "~1.0",
        "michelf/php-markdown": "~1.4",
        "nickcernis/html-to-markdown": "~2.1",
        "misd/linkify": "~1.1@dev",
        "hashids/hashids": "~0.3@dev"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "codeception/codeception": "~2.0",
        "codeception/aspect-mock": "~0.4"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/database/migrations",
            "app/database/seeds",
            "tests/unit/UnitTestCase.php"
        ],
        "psr-0": {
            "Api\\": "app/controllers/"
        },
        "psr-4": {
            "Lio\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
        ],
        "pre-update-cmd": [
        ],
        "post-update-cmd": [
        ],
        "post-create-project-cmd": [
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "RC"
}
